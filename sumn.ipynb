    "def add():\n",
    "    n=int(input(\" enter number to print sum of n numbers\"))\n",
    "    i=0\n",
    "    sum=0\n",
    "    while(i<=n):\n",
    "        sum+=i\n",
    "        i+=1\n",
    "    return sum        \n",
